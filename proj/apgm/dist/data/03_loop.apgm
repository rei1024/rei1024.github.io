/*
    # Infinite loop

    ## syntax
    loop <expression>

    break(<optional number>)
*/

loop {
    output("2");
    break(); /* break from loop */
}

/* break level */
loop { /* outer loop */
    loop { /* inner loop */
        break(2); /* break from outer loop */
    }
}

output("3");

/* infinite loop */
loop {
    output(".");
}