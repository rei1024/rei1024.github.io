// increment U0
inc_u(0);

// tdec_u(0) = TDEC U0
if_zero(tdec_u(0)) {
    // execute if `tdec_u(0)` returns Z
    output("1");
} else {
    // execute if `tdec_u(0)` returns NZ
    output("2");
}

// tdec_u(1) = TDEC U1
if_non_zero(tdec_u(1)) {
    // execute if `tdec_u(1)` returns NZ
    output("3");
} else {
    // execute if `tdec_u(1)` returns Z
    output("4");
}
